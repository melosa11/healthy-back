{
    "permissions": [
        "notifications",
        "alarms",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
        "page": "settings/settings-settings.html", 
        "browser_style": false  
    },
    "browser_action": {
        "default_icon": "icons/logo.svg",
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "48": "icons/logo.svg",
        "96": "icons/logo.svg"
    },

    "name": "Healthy Back",
    "author": "David Sutor",
    "description": "Healthy Back reminds you to sit up straight",
    
    "manifest_version": 2,

    "version": "1.0.1.0"
}